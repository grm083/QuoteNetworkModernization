<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ValidateEndDateBeyondBundleEndDate</fullName>
    <active>true</active>
    <description>SDT-41184-Validation for End Date</description>
    <errorConditionFormula>NOT (ISPICKVAL(SBQQ__Quote__r.SBQQ__Type__c, &apos;Quote&apos;)) &amp;&amp;(SBQQ__EndDate__c &gt; SBQQ__RequiredBy__r.SBQQ__EndDate__c )</errorConditionFormula>
    <errorDisplayField>SBQQ__EndDate__c</errorDisplayField>
    <errorMessage>End Date cannot be beyond End Date of the Bundle</errorMessage>
</ValidationRule>
