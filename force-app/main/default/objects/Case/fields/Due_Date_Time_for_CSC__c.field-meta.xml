<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Due_Date_Time_for_CSC__c</fullName>
    <description>This is to built, due date time for Confirm service completion task. It should be service date + 1 day with 1 PM EST</description>
    <externalId>false</externalId>
    <formula>DATETIMEVALUE(Text(Service_Date__c+1)+&quot; 17:00:00&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Due Date Time for CSC</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>DateTime</type>
</CustomField>
