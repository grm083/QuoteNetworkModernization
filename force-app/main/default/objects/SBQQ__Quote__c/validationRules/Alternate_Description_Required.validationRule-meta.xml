<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Alternate_Description_Required</fullName>
    <active>true</active>
    <errorConditionFormula>AND(Alternate_Service_Provided__c  = True, ISBLANK(Alternate_Description__c))</errorConditionFormula>
    <errorDisplayField>Alternate_Description__c</errorDisplayField>
    <errorMessage>Alternate Description can not be blank</errorMessage>
</ValidationRule>
