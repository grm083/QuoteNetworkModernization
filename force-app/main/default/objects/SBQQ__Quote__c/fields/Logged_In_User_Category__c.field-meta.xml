<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Logged_In_User_Category__c</fullName>
    <description>SDT-41186-Created the field to store the type of user who logged in. E.g. SALESGEN and SALESPREM are categorized as SALES users.</description>
    <externalId>false</externalId>
    <formula>IF((ISPICKVAL($User.User_categorization_code__c, &apos;SALESGEN&apos;) || ISPICKVAL($User.User_categorization_code__c, &apos;SALESPREM&apos;)), &apos;SALES&apos;, IF(ISPICKVAL($User.User_categorization_code__c, &apos;SSMGEN&apos;), &apos;SSM&apos;,
IF(ISPICKVAL($User.User_categorization_code__c, &apos;CSRES&apos;), &apos;SST&apos;, &apos;NONE&apos;)))</formula>
    <label>Logged In User Category</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
