<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Prevent_Asset_Name_Change</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT($User.Bypass_Validation__c), ISCHANGED(Name))</errorConditionFormula>
    <errorMessage>Asset Name is not an editable field.</errorMessage>
</ValidationRule>
