<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Monthly_Frequency__c</fullName>
    <externalId>false</externalId>
    <formula>IF( ISPICKVAL(Occurrence_Type__c, &quot;Scheduled&quot;),
CASE(Frequency__c,
&quot;Daily&quot;,30/VALUE(Occurs__c),
&quot;Monthly&quot;, VALUE(Occurs__c)/12,
&quot;Yearly&quot;, VALUE(Occurs__c)/12, 1), 1)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Monthly Frequency</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>