<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Notification_Phone__c</fullName>
    <businessStatus>DeprecateCandidate</businessStatus>
    <description>Formula field to fetch Contact MobilePhone field if that is null fetch Phone  field and add &apos;1&apos; at the beginning of the number</description>
    <externalId>false</externalId>
    <formula>IF(
	ISBLANK(Contact_Name__r.MobilePhone),
	IF(ISBLANK(Contact_Name__r.Phone),&quot;&quot; , &quot;1&quot; &amp; Contact_Name__r.Phone),
	&quot;1&quot; &amp; Contact_Name__r.MobilePhone
)</formula>
    <label>Notification Phone</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
