<aura:component implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,force:lightningQuickActionWithoutHeader,lightning:actionOverride" controller="ButtonAccessController" access="global" >
	<aura:handler name="init" action="{!c.doInit}" value="{!this}" />
<aura:attribute name="noAccess" type="Boolean" default="false" />
    <lightning:workspaceAPI aura:id="workspace"/>

        <aura:if isTrue="{!v.noAccess}">
        <div class="slds-card slds-align_absolute-center">
            <div class="slds-p-around_medium">
                <div class="slds-text-heading_large">
                    <h1 class="slds-align_absolute-center slds-text-color_error">You Don't Have Access To Create Pricing Request</h1>
                </div>
            </div>
        </div>
    </aura:if>
</aura:component>