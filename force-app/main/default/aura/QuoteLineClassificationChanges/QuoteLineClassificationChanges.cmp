                            <tr style="background-color: #f2f2f2">
                                <th>Service</th>
                                <th>Schedule</th>
                                <th>Cost</th>
                                <th>Price</th>
                                <th>Start Date</th>
                                <th>End Date</th>
                                <th>Vendor</th>
                            </tr>
